print a number is positive or negative or zero
#include <stdio.h>

int main() {
    int a;
    scanf("%d",&a);
    if(a>0)
    {
        printf("positive number");
    }
    else
    if(a<0)
    {
        printf("negative number");
    }
    else 
    {
        printf("zero");
    }
    

    return 0;
}
